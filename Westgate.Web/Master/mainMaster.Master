        function OnSearchClick() {
            var search = document.getElementById("searchfield").value;
            if (search == null) {
                search = "";
            }
            $.post('ajax/SearchContents.aspx?search=' + search, function (data) {
                $('#contentsDiv').html(data);
            });
            $("#imagesFrame").hide();
            $("#profileSection").hide();
            var width = $("#mainDiv").width();
            $("#profilemain").attr("style", "width: 0%; float: right; min-height: 375px;");
            $("#contentsDiv").attr("style", "padding-top: 10px; min-height: 363px; float:left;");
            $("#contentsDiv").width(width - 30);
            $("#loadingDiv").attr("style", "width: 100%; min-height: 500px; background-color: #f6f6f6; display:none;");
        }

        function SendEmail() {
            var companyNameVal = $("#<%= CompanyTextID.ClientID%>").val();
            var personNameVal = $("#<%= PersonTextID.ClientID%>").val();
            var emailtextVal = $("#<%= EmailTextID.ClientID%>").val();
            var postCodeVal = $("#<%= PostTextID.ClientID%>").val();
            var phoneNumberVal = $("#<%= PhoneTextID.ClientID%>").val();
            var enquiryVal = $("#<%= EnquirySelectedId.ClientID%>").val();
            $.ajax({
                type: "POST",
                url: "ajax/AddUserEnquiery.aspx",
                data: { companyName: companyNameVal, personName: personNameVal, email: emailtextVal, postCode: postCodeVal, phoneNumber: phoneNumberVal, enquiry: enquiryVal },
                success: onSuccess,
                fail: onFail
            });
            return false;
        }



        function onSuccess(response) {
            alert("Message sent sucessfully.");
            $("#<%= CompanyTextID.ClientID%>").val("");
            $("#<%= PersonTextID.ClientID%>").val("");
            $("#<%= EmailTextID.ClientID%>").val("");
            $("#<%= PostTextID.ClientID%>").val("");
            $("#<%= PhoneTextID.ClientID%>").val("");
        }

        function onFail(response) {
        }

        function detectKey(e, t) {
            var keynum;
            if (window.event) // IE
            {
                keynum = e.keyCode;
            }
            else if (e.which) // Netscape/Firefox/Opera
            {
                keynum = e.which;
            }
            if (keynum == 13) {
                t.blur();
            }
        }

    </script>
